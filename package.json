  "version": "1.0",
  "description": "This is my portfolio site using Gatsby.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nouvelle/portfolio"
  },
  "author": {
    "name": "Eriko"
  },
  "scripts": {
    "develop": "gatsby develop",
