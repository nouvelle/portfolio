    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-images": "^1.1.0",
    "react-scrollspy": "^3.4.2",
    "smoothscroll-polyfill": "^0.4.4"
